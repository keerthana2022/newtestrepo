apiVersion: apps/v1
kind: Deployment
metadata:
   name: tomdeploy
   labels:
     app: web
spec:
       replicas: 2
       selector:
        matchLabels:
         app: tom
       template:
        metadata:
          name: tompod
          labels:
            app: tom
        spec:
         containers:
          - name: tomcontainer
            image: keerthana2022/tom:version6
            ports:
               - containerPort: 8080


---
apiVersion: v1
kind: Service
metadata:
  name: tom-svc
spec:
  ports:
  - port: 8080
    targetPort: 8080
    nodePort: 31000
  selector:
    app: tom
  type: NodePort
